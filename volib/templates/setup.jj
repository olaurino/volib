from setuptools import setup, find_packages

setup(
name="{{ meta.name }}",
version="{{ meta.version }}",
packages=find_packages(),
entry_points="""
[vodml.resolver]
{% for model in models -%}
{{ model[0] }},{{ model[1] }}={{ meta.name|alpha }}.{{ model[0]|alpha }}_{{ model[1]|alpha }}.aux:resolver
{% endfor -%}
""",
)